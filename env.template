# Copy this file to .env.local and fill in your actual values

# Supabase Auth Configuration (already included above)
# No additional auth environment variables needed

# Supabase Configuration
NEXT_PUBLIC_SUPABASE_URL=your-supabase-url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key

# Stripe Configuration
STRIPE_PUBLISHABLE_KEY=your-stripe-publishable-key
STRIPE_SECRET_KEY=your-stripe-secret-key
STRIPE_WEBHOOK_SECRET=your-stripe-webhook-secret

# AI Services
OPENROUTER_API_KEY=your-openrouter-api-key
HUGGINGFACE_API_KEY=your-huggingface-api-key

# Database
DATABASE_URL=your-supabase-database-url

# Email Configuration (optional - Supabase handles auth emails by default)
# Only needed if you want custom email templates
EMAIL_FROM=noreply@paradoc.app
